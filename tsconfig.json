{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2022",
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@bot/*": ["bot/*"],
      "@services/*": ["services/*"],
      "@queue/*": ["queue/*"],
      "@configs/*": ["configs/*"]
    }
  }
}